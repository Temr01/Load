<Page x:Class="Load.Pages.SocPasport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Load.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Социальный паспорт" FontSize="18" Width="910" Height="650" Loaded="Page_Loaded">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <ComboBox x:Name="Group" Width="400" Margin="10" DisplayMemberPath="name"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Border BorderThickness="3" CornerRadius="10" Margin="5,5,20,5" Background="#FF03780D">
                    <Button x:Name="Go" Content="Печать" FontSize="20" Width="100"  BorderThickness="0" Background="{x:Null}" Click="Go_Click" />
                </Border>
                <Border BorderThickness="3" CornerRadius="10" Margin="5,5,20,5" Background="#FF03780D">
                    <Button x:Name="Cancel" Content="Отмена" FontSize="20" Width="100"  BorderThickness="0" Background="{x:Null}"/>
                </Border>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
